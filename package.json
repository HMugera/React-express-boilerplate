{
  "name": "react-express-boilerplate",
  "version": "1.0.0",
  "description": "biolerplate for react and express application",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": " npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "hughes mugera",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
